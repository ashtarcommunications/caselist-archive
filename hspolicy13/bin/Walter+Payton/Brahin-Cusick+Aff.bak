return false;
}
}

var delroundurl = window.location.protocol + "//" + window.location.host + "/" + "rest/wikis/hspolicy/spaces/Walter Payton/pages/Brahin-Cusick Aff/objects/Caselist.RoundClass/" + roundobjnum;

