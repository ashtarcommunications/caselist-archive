if (r==false) {
return false;
}
}

var delroundurl = window.location.protocol + "//" + window.location.host + "/" + "rest/wikis/opencaselist13/spaces/Michigan State/pages/Gannon-Hodgman Aff/objects/Caselist.RoundClass/" + roundobjnum;

var client = new XMLHttpRequest();
client.open("DELETE", delroundurl, false);
client.send();

if (client.status == 204) {
if (silent != true) {
alert("Round Deleted Successfully!");
} else {
console.log("Round Deleted Successfully!");
}
} else {
